  <properties>
    <java.version>1.8</java.version>
    <maven.version>3.6.0</maven.version>
    <!-- sonar settings -->
    <sonar.host.url>http://vento:9000</sonar.host.url>
    <sonar.jdbc.url>jdbc:postgresql://habub/sonar</sonar.jdbc.url>
    <!-- version properties -->
    <deegree3.version>3.4.10-xplanbox-1.14</deegree3.version>
    <geowerkstatt-hamburg.xplanung.version>0.9.14</geowerkstatt-hamburg.xplanung.version>
