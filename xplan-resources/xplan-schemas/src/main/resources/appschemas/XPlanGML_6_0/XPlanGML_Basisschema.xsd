  <!--XP_VertikaleAusrichtung-->
  <!--=========================================================================================-->
  <simpleType name="XP_VertikaleAusrichtung">
    <annotation>
      <documentation>Enumeration der definierten vertikalen Fontausrichtungen
</documentation>
    </annotation>
