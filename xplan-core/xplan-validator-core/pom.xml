    <artifactId>xplan-core</artifactId>
    <version>5.0.3-7ff28682-2</version>
  </parent>

  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>jaxb2-maven-plugin</artifactId>
        <configuration>
          <xjbSources>
            <xjbSource>${project.basedir}/src/xjc/bindings.xjc</xjbSource>
          </xjbSources>
          <sources>
            <source>${project.basedir}/src/main/resources/schemas</source>
          </sources>
          <outputDirectory>${project.build.directory}/generated-sources/jaxb</outputDirectory>
          <packageName>de.latlon.xplan.validator.report</packageName>
        </configuration>
      </plugin>
    </plugins>
  </build>

