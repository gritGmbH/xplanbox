<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='xplanmgrweb'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/>   -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name='de.latlon.xplan.commons.XPlanCommonsWeb'/>
  <inherits name='de.latlon.xplan.validator.web.XPlanValidatorWebCommons'/>
  <inherits name='org.gwtopenmaps.openlayers.OpenLayers'/>
  <inherits name='org.fusesource.restygwt.RestyGWT' />
  <script src="js/OpenLayers-2.7/OpenLayers.js"></script>
  <script src="js/proj4/proj4js/lib/proj4js-compressed.js"></script>
  <script src="js/proj4/proj4js/lib/defs/EPSG25832.js"></script>
  <script src="js/proj4/proj4js/lib/defs/EPSG25833.js"></script>
  <script src="js/proj4/proj4js/lib/defs/EPSG31466.js"></script>
  <script src="js/proj4/proj4js/lib/defs/EPSG31467.js"></script>
  <script src="js/proj4/proj4js/lib/defs/EPSG31468.js"></script>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='de.latlon.xplan.manager.web.client.gui.ManagerWebEntryPoint'/>

  <!-- i18n -->
  <extend-property name="locale" values="de" />

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

  <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
  <set-property name="user.agent" value="ie8,ie9,ie10,gecko1_8,safari,opera"/>
</module>
