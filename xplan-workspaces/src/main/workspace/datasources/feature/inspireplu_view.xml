<?xml version='1.0' encoding='UTF-8'?>
<SQLFeatureStore configVersion="3.4.0"
                 xmlns="http://www.deegree.org/datasource/feature/sql"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:app="http://www.deegree.org/app"
                 xsi:schemaLocation="http://www.deegree.org/datasource/feature/sql http://schemas.deegree.org/datasource/feature/sql/3.4.0/sql.xsd">
  <JDBCConnId>inspireplu</JDBCConnId>

  <FeatureTypeMapping name="app:SpatialPlan" table="VIEWSERVICE_SPATIALPLAN">
    <FIDMapping prefix="SPATIALPLAN_">
      <Column name="spatialplan_gml_id" type="string"/>
      <UUIDGenerator/>
    </FIDMapping>
    <Geometry path="extent" mapping="extent" type="Point">
      <StorageCRS srid="-1" dim="2D">${vectorCrs}</StorageCRS>
    </Geometry>
    <Primitive path="officialtitle" mapping="officialtitle" type="string"/>
  </FeatureTypeMapping>

  <FeatureTypeMapping name="app:SupplementaryRegulation" table="VIEWSERVICE_SUPPLEMENTARY_REGULATION">
    <FIDMapping prefix="SUPPLEMENTARY_REGULATION_">
      <Column name="supplementaryregulation_gml_id" type="string"/>
      <UUIDGenerator/>
    </FIDMapping>
    <Geometry path="geometry" mapping="geometry" type="Point">
      <StorageCRS srid="-1" dim="2D">${vectorCrs}</StorageCRS>
    </Geometry>
    <Primitive path="supplementaryregulations" mapping="supplementaryregulations" type="string"/>
  </FeatureTypeMapping>

  <FeatureTypeMapping name="app:ZoningElement" table="VIEWSERVICE_ZONING_ELEMENT">
    <FIDMapping prefix="ZONING_ELEMENT_">
      <Column name="zoningelement_gml_id" type="string"/>
      <UUIDGenerator/>
    </FIDMapping>
    <Geometry path="geometry" mapping="geometry" type="Point">
      <StorageCRS srid="-1" dim="2D">${vectorCrs}</StorageCRS>
    </Geometry>
    <Primitive path="hilucs" mapping="hilucs" type="string"/>
  </FeatureTypeMapping>

</SQLFeatureStore>