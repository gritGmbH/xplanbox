    </td>
</tr>

<?template makeitalink>
<tr>
  <td <?odd:maybegray>><?name:map propname></td>
  <td <?odd:maybegray>><a target="_blank" href="http://localhost:8080/xplan-wms<?value>">Dokument herunterladen</a></td>
</tr>

<?template property>
<tr>
  <td <?even:maybegray> valign="top" style="width:20%;">
        <strong> 
