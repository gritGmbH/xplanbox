<?template start>
<html>
  <head>
    <title>deegree XPlan WMS Sachdatenanzeige</title>
    <meta http-equiv='Content-Type' content='text/html; charset=utf-8'>
  </head>
  
  <body>
    <script>
        window.onload = function () {
          var complexProperties = document.getElementsByClassName('complexProperty');
          for(var i = 0; i < complexProperties.length; i++) {
            complexProperties[i].innerHTML = complexProperties[i].innerHTML.replace(/\|/g, '<br/>');
          }
        }
      </script>
    <h2>XPlanWMS: Sachdaten</h2>
    <table style='font-family:Arial; font-size: 0.9em;'>
      <?feature *:feature>
    </table>
  </body>
</html>

<?template feature>
<tr style="background-color: white;">
      <td colspan="2" style="font-size: 1.1em; border-bottom: solid black 1px; padding-top: 10px; padding-bottom: 5px;">
		<strong>
            <?name:map ftname>
		</strong>
      </td>
</tr>
<?property not(wirdDargestelltDurch,gmlId,raster*,ref*,fachobjekt,flaechenteil,*Code,gehoert*,begrenzungslinie,uuid,bereich,wmsSortDate,praesentationsobjekt,gemeinde,hoehenangabe,aendert,wurdeGeaendertVon,dachgestaltung,massnahme):property>
