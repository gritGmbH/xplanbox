  <!-- Wohnbauflaeche -->
  <Rule>
    <Name>Wohnbauflaeche</Name>
    <Description>
      <Title>FP: Wohnbaufläche</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:allgArtDerBaulNutzungCode</ogc:PropertyName>
          <ogc:Literal>1000</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:Or>
          <ogc:PropertyIsNull>
            <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
          </ogc:PropertyIsNull>
          <ogc:Not>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
              <ogc:Literal>true</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Not>
        </ogc:Or>
      </ogc:And>
    </ogc:Filter>
    <MaxScaleDenominator>100000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <SvgParameter name="fill">#ff8a67</SvgParameter>
        <SvgParameter name="fill-opacity">1.0</SvgParameter>
      </Fill>
      <Stroke>
        <SvgParameter name="stroke">#000000</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">1.0</SvgParameter>
        <SvgParameter name="stroke-dasharray">1</SvgParameter>
      </Stroke>
    </PolygonSymbolizer>
  </Rule>

