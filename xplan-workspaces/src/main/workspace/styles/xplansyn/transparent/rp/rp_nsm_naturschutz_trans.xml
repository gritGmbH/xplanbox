<sld:StyledLayerDescriptor version="1.0.0" xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xplan="http://www.deegree.org/xplanung/1/0">
  <sld:NamedLayer>
    <sld:Name>RP_NSM_Naturschutz</sld:Name>
    <sld:UserStyle>
      <sld:Name>transparent</sld:Name>
      <sld:FeatureTypeStyle>
        <sld:FeatureTypeName>xplan:RP_NSM_Naturschutz</sld:FeatureTypeName>

        <!--GruenlandBewirtschaftungPflegeEntwicklung-->

        <sld:Rule>
          <sld:Name>vb_Gruenland</sld:Name>
          <sld:Title>Vorbehaltsgebiet Gr�nlandbewirtschaftung</sld:Title>
          <ogc:Filter>
            <ogc:And>
              <ogc:PropertyIsEqualTo>
                <ogc:PropertyName>xplan:gebietsTypCode</ogc:PropertyName>
                <ogc:Literal>2000</ogc:Literal>
              </ogc:PropertyIsEqualTo>
              <ogc:PropertyIsEqualTo>
                <ogc:PropertyName>xplan:typNS_NSMCode</ogc:PropertyName>
                <ogc:Literal>1000</ogc:Literal>
              </ogc:PropertyIsEqualTo>
            </ogc:And>
          </ogc:Filter>
          <sld:PolygonSymbolizer>
            <Geometry>
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <sld:Fill>
              <sld:CSSParameter name="fill-opacity">0</sld:CSSParameter>
            </sld:Fill>
            <sld:Stroke>
              <sld:CssParameter name="stroke">#78DC00</sld:CssParameter>
              <sld:CssParameter name="stroke-width">1</sld:CssParameter>
              <sld:CssParameter name="stroke-opacity">0</sld:CssParameter>
            </sld:Stroke>
          </sld:PolygonSymbolizer>
        </sld:Rule>

        <sld:Rule>
          <sld:Name>VR_Gruenland</sld:Name>
          <sld:Title>Vorranggebiet Gr�nlandbewirtschaftung</sld:Title>
