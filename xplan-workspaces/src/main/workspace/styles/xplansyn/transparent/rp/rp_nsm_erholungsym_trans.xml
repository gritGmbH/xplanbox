<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<sld:StyledLayerDescriptor version="1.0.0" xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xplan="http://www.deegree.org/xplanung/1/0">
  <sld:NamedLayer>
    <sld:Name>RP_NSM_Erholung</sld:Name>
    <sld:UserStyle>
      <sld:Name>transparent</sld:Name>
      <sld:FeatureTypeStyle>
        <sld:FeatureTypeName>xplan:RP_NSM_Erholung</sld:FeatureTypeName>


        <!--SchwerpunktbereichErholung-->

        <sld:Rule>
          <sld:Name>Erholungsschwerpunkt</sld:Name>
          <sld:Title>Erholungsschwerpunkt</sld:Title>
          <sld:PointSymbolizer>
            <Geometry>
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <sld:Graphic>
              <sld:Mark>
            <sld:Fill>
              <sld:CSSParameter name="fill-opacity">0</sld:CSSParameter>
            </sld:Fill>
            <sld:Stroke>
              <sld:CSSParameter name="stroke-opacity">0</sld:CSSParameter>
            </sld:Stroke>
              </sld:Mark>
            </sld:Graphic>
          </sld:PointSymbolizer>
        </sld:Rule>


        <!--andere-->

        <sld:Rule>
          <sld:Name>ruhigeErholung</sld:Name>
          <sld:Title>Vorranggebiet ruhige Erholung</sld:Title>
          <sld:PointSymbolizer>
            <Geometry>
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
          </sld:PointSymbolizer>
        </sld:Rule>

        <sld:Rule>
          <sld:Name>intensiveErholung</sld:Name>
          <sld:Title>Vorranggebiet intensive Erholung</sld:Title>
          <sld:PointSymbolizer>
            <Geometry>
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <sld:Graphic>
              <sld:Mark>
            <sld:Fill>
              <sld:CSSParameter name="fill-opacity">0</sld:CSSParameter>
            </sld:Fill>
            <sld:Stroke>
              <sld:CSSParameter name="stroke-opacity">0</sld:CSSParameter>
            </sld:Stroke>
              </sld:Mark>
            </sld:Graphic>
          </sld:PointSymbolizer>
        </sld:Rule>

        <sld:Rule>
          <sld:Name>StandortErholung</sld:Name>
          <sld:Title>Standort Erholung</sld:Title>
          <sld:PointSymbolizer>
            <Geometry>
              <ogc:PropertyName>xplan:position</ogc:PropertyName>
            </Geometry>
            <sld:Graphic>
              <sld:Mark>
            <sld:Fill>
              <sld:CSSParameter name="fill-opacity">0</sld:CSSParameter>
            </sld:Fill>
            <sld:Stroke>
              <sld:CSSParameter name="stroke-opacity">0</sld:CSSParameter>
            </sld:Stroke>
          </sld:Mark>
        </sld:Graphic>
          </sld:PointSymbolizer>
        </sld:Rule>


      </sld:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>

