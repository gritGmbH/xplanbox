<?xml version="1.0" encoding="UTF-8"?>
<FeatureTypeStyle xmlns="http://www.opengis.net/se"
                  xmlns:ogc="http://www.opengis.net/ogc"
                  xmlns:xplan="http://www.deegree.org/xplanung/1/0"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd">
  <Name>transparent</Name>
  <FeatureTypeName>xplan:RP_Funktionszuweisung</FeatureTypeName>

  <!--RP_Funktionszuweisung-->
  <Rule>
    <Name>RP_Funktionszuweisung</Name>
    <Description>
      <Title>Funktionszuweisung</Title>
    </Description>

    <PolygonSymbolizer>
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <SvgParameter name="fill-opacity">0</SvgParameter>
      </Fill>
      <Stroke>
        <SvgParameter name="stroke-opacity">0</SvgParameter>
      </Stroke>
    </PolygonSymbolizer>
  </Rule>

</FeatureTypeStyle>
