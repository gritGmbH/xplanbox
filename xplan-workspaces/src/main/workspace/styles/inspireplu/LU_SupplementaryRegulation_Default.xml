<se:FeatureTypeStyle version="1.1.0"
                     xmlns:se="http://www.opengis.net/se"
                     xmlns:ogc="http://www.opengis.net/ogc"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xmlns:app="http://www.deegree.org/app"
                     xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd">
  <se:Name>LandUse.SupplementaryRegulation.Default</se:Name>
  <se:Description>
    <se:Title>Supplementary Regulation Default Style</se:Title>
    <se:Abstract>Contour or line of the Supplementary regulation depicted by a colour line of two pixels,
      colour depending of the value of the attribute supplementaryRegulation...
    </se:Abstract>
  </se:Description>
  <se:FeatureTypeName>app:SupplementaryRegulation</se:FeatureTypeName>

  <!--ImpactOnEnvironment-->
  <se:Rule>
    <se:Name>Impact on Environment</se:Name>
    <ogc:Filter>
      <ogc:PropertyIsLike wildCard="*" singleChar="." escapeChar="!">
        <ogc:PropertyName>app:supplementaryregulations</ogc:PropertyName>
        <ogc:Literal>*http://inspire.ec.europa.eu/codelist/SupplementaryRegulationValue/1_*</ogc:Literal>
      </ogc:PropertyIsLike>
    </ogc:Filter>
    <se:LineSymbolizer>
      <se:Geometry>
        <ogc:PropertyName>app:geometry</ogc:PropertyName>
      </se:Geometry>
      <se:Stroke>
        <se:SvgParameter name="stroke">#b4e66e</se:SvgParameter>
        <se:SvgParameter name="stroke-width">2</se:SvgParameter>
      </se:Stroke>
    </se:LineSymbolizer>
  </se:Rule>
  <!--RiskExposure-->
