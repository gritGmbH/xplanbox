<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            configVersion="3.2.0"
            xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/services/wfs/3.2.0/wfs_configuration.xsd">

  <!-- [0...1] Activated OGC protocol versions, default: activate all implemented versions -->
  <SupportedVersions>
    <Version>1.1.0</Version>
    <Version>2.0.0</Version>
  </SupportedVersions>

  <FeatureStoreId>xplan40</FeatureStoreId>
  <FeatureStoreId>xplan41</FeatureStoreId>
  <!-- [0...1] Enable transactions (WFS-T operations), default: disabled -->

  <EnableTransactions>false</EnableTransactions>

  <!--
    [0...1] Disable response buffering (allows rollback for generating clean exception reports, but hinders streaming),
    default: true
  -->
  <DisableResponseBuffering>true</DisableResponseBuffering>

  <QueryCRS>${vectorCrs}</QueryCRS>
  <QueryCRS>EPSG:25832</QueryCRS>
  <QueryCRS>EPSG:25833</QueryCRS>
  <QueryCRS>EPSG:31466</QueryCRS>
  <QueryCRS>EPSG:31467</QueryCRS>
  <QueryCRS>EPSG:31468</QueryCRS>
  <QueryCRS>EPSG:31469</QueryCRS>
  <QueryCRS>EPSG:4258</QueryCRS>
  <QueryCRS>EPSG:4326</QueryCRS>
  <QueryCRS>EPSG:4839</QueryCRS>
  <QueryCRS>CRS:84</QueryCRS>
  
  <!-- StoredQuery -->
  <StoredQuery>storedqueries/planName.xml</StoredQuery>
  <StoredQuery>storedqueries/planId.xml</StoredQuery>
  <StoredQuery>storedqueries/internalId.xml</StoredQuery>

    <GMLFormat gmlVersion="GML_32">
        <MimeType>text/xml; subtype=gml/3.2.1</MimeType>
        <GenerateBoundedByForFeatures>false</GenerateBoundedByForFeatures>
        <GetFeatureResponse xmlns:gml="http://www.opengis.net/gml/3.2">
            <ContainerElement>gml:FeatureCollection</ContainerElement>
            <FeatureMemberElement>gml:featureMember</FeatureMemberElement>
            <AdditionalSchemaLocation>>http://www.opengis.net/gml/3.2
                http://www.xplanungwiki.de/upload/XPlanGML/4.1-Kernmodell/Schema/gmlProfile/gmlProfilexplan.xsd
            </AdditionalSchemaLocation>
            <DisableStreaming>false</DisableStreaming>
        </GetFeatureResponse>
    </GMLFormat>

</deegreeWFS>