          <ogc:Literal>3000</ogc:Literal>
        </ogc:PropertyIsEqualTo>
      </ogc:And>
    </ogc:Filter>
    <PointSymbolizer>
      <Graphic>
        <ExternalGraphic>
          <OnlineResource xlink:type="simple" xlink:href="../symbols/RROP/RROP_S_Motorsport.svg"/>
          <Format>image/svg</Format>
        </ExternalGraphic>
        <Size>
          <ogc:Literal>52</ogc:Literal>
        </Size>
        <Rotation>0</Rotation>
      </Graphic>
    </PointSymbolizer>
  </Rule>


