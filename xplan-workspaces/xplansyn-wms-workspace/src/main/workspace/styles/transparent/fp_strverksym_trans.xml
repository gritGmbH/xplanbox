  <!-- Durchgang Punkt -->
  <Rule>
    <Name>Durchgang</Name>
    <Description>
      <Title>FP: Durchgang</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:Not>
          <ogc:PropertyIsNull>
            <ogc:PropertyName>xplan:symbolPosition</ogc:PropertyName>
          </ogc:PropertyIsNull>
        </ogc:Not>
        <ogc:PropertyIsEqualTo>
          <ogc:Function name="IsPoint">
            <ogc:PropertyName>xplan:SymbolPosition</ogc:PropertyName>
          </ogc:Function>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:zweckbestimmungCode</ogc:PropertyName>
          <ogc:Literal>XP2_1500</ogc:Literal>
        </ogc:PropertyIsEqualTo>
      </ogc:And>
    </ogc:Filter>
    <MaxScaleDenominator>10000</MaxScaleDenominator>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:SymbolPosition</ogc:PropertyName>
      </Geometry>
      <Graphic>
        <Mark>
          <WellKnownName>square</WellKnownName>
          <Fill>
            <SvgParameter name="fill">#000000</SvgParameter>
            <SvgParameter name="fill-opacity">0</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke">#000000</SvgParameter>
            <SvgParameter name="stroke-width">0.5</SvgParameter>
            <SvgParameter name="stroke-opacity">0</SvgParameter>
          </Stroke>
        </Mark>
        <Size>70</Size>
      </Graphic>
    </PointSymbolizer>
    <TextSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:SymbolPosition</ogc:PropertyName>
      </Geometry>
      <Label>
        <ogc:Literal>Du</ogc:Literal>
      </Label>
      <Font>
        <SvgParameter name="font-family">Arial</SvgParameter>
        <SvgParameter name="font-family">Sans-Serif</SvgParameter>
        <SvgParameter name="font-weight">bold</SvgParameter>
        <SvgParameter name="font-size">50</SvgParameter>
      </Font>
      <LabelPlacement>
        <PointPlacement>
          <Displacement>
            <DisplacementX>-3</DisplacementX>
            <DisplacementY>0</DisplacementY>
          </Displacement>
        </PointPlacement>
      </LabelPlacement>
      <Fill>
        <SvgParameter name="fill">#FFFFFF</SvgParameter>
        <SvgParameter name="fill-opacity">0</SvgParameter>
      </Fill>
    </TextSymbolizer>
  </Rule>

  <!-- detailierte Zweckbestimmung -->
  <Rule>
    <Name>detaillierteZweckbestimmung</Name>
    <Description>
      <Title>detaillierte Zweckbestimmung</Title>
    </Description>
    <ogc:Filter>
      <ogc:Not>
        <ogc:PropertyIsNull>
          <ogc:PropertyName>xplan:detaillierteZweckbestimmung</ogc:PropertyName>
        </ogc:PropertyIsNull>
      </ogc:Not>
    </ogc:Filter>
    <MaxScaleDenominator>10000</MaxScaleDenominator>
    <TextSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Label>
        <ogc:PropertyName>xplan:detaillierteZweckbestimmung</ogc:PropertyName>
      </Label>
      <Font>
        <SvgParameter name="font-family">Arial</SvgParameter>
        <SvgParameter name="font-family">Sans-Serif</SvgParameter>
        <SvgParameter name="font-weight">bold</SvgParameter>
        <SvgParameter name="font-size">50</SvgParameter>
      </Font>
      <LabelPlacement>
        <PointPlacement>
          <Displacement>
            <DisplacementX>9</DisplacementX>
            <DisplacementY>0</DisplacementY>
          </Displacement>
        </PointPlacement>
      </LabelPlacement>
      <Fill>
        <SvgParameter name="fill">#000000</SvgParameter>
        <SvgParameter name="fill-opacity">0</SvgParameter>
      </Fill>
    </TextSymbolizer>
  </Rule>
