<?xml version="1.0" encoding="UTF-8"?>
<FeatureTypeStyle xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.opengis.net/se"
  xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:xplan="http://www.deegree.org/xplanung/1/0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd">
  <Name>vollflaechig</Name>
  <FeatureTypeName>xplan:LP_WasserrechtSchutzgebiet</FeatureTypeName>
  <!-- Fläche -->
  <Rule>
    <Name>lp_wrschutzgeb1</Name>
    <Description>
      <Title>Schutzgebiet für Grund- und Quellwassergewinnung</Title>
    </Description>
    <ogc:Filter>

      <ogc:PropertyIsEqualTo>
        <ogc:PropertyName>xplan:typ</ogc:PropertyName>
        <ogc:Literal>1000</ogc:Literal>
      </ogc:PropertyIsEqualTo>

    </ogc:Filter>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:Function name="Centroid">
          <ogc:PropertyName>xplan:symbolPosition</ogc:PropertyName>
        </ogc:Function>
      </Geometry>
      <Graphic>
        <Mark>
          <WellKnownName>circle</WellKnownName>
          <Fill>
            <SvgParameter name="fill">#FFFFFF</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke">#000000</SvgParameter>
            <SvgParameter name="stroke-opacity">1.0</SvgParameter>
            <SvgParameter name="stroke-width">1</SvgParameter>
          </Stroke>
        </Mark>
        <Size>50</Size>
      </Graphic>
    </PointSymbolizer>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:symbolPosition</ogc:PropertyName>
      </Geometry>
      <Graphic>
        <Mark>
          <OnlineResource xlink:href="../symbols/FreeSans.ttf" />
          <Format>ttf</Format>
          <MarkIndex>42</MarkIndex>
          <Fill>
            <SvgParameter name="fill">#000000</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke-opacity">0</SvgParameter>
          </Stroke>
        </Mark>
        <Size>20</Size>
        <Displacement>
          <DisplacementX>-3</DisplacementX>
          <DisplacementY>0</DisplacementY>
        </Displacement>
      </Graphic>
    </PointSymbolizer>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:symbolPosition</ogc:PropertyName>
      </Geometry>
      <Graphic>
        <Mark>
          <OnlineResource xlink:href="../symbols/FreeSans.ttf" />
          <Format>ttf</Format>
          <MarkIndex>58</MarkIndex>
          <Fill>
            <SvgParameter name="fill">#000000</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke-opacity">0</SvgParameter>
          </Stroke>
        </Mark>
        <Size>20</Size>
        <Displacement>
          <DisplacementX>3</DisplacementX>
          <DisplacementY>0</DisplacementY>
        </Displacement>
      </Graphic>
    </PointSymbolizer>
  </Rule>
